{
  "name": "ng1-decor",
  "version": "0.9.0",
  "description": "Typescript Decorators to Angular 1.5",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --env jest-environment-node-debug",
    "coverage": "rimraf coverage && npm test -- --coverage --no-cache",
    "release": "npm pack"
  },
  "keywords": [
    "ts",
    "typescript",
    "decorator",
    "decorators",
    "angular"
  ],
  "author": "Cervantes",
  "license": "ISC",
  "devDependencies": {
    "@types/angular": "^1.5.8",
    "@types/jest": "^18.1.1",
    "@types/node": "^7.0.5",
    "angular": "1.5.8",
    "awesome-typescript-loader": "^3.0.3",
    "harmony": "0.0.1",
    "jest": "^18.1.0",
    "rimraf": "^2.5.4",
    "ts-jest": "^18.0.3",
    "tslint": "^4.4.2",
    "typescript": "^3.2.2",
    "webpack-node-externals": "^1.5.4"
  },
  "peerDependencies": {
    "angular": ">=1.5.0",
    "typescript": ">=2.1.0"
  },
  "jest": {
    "transform": {
      ".(ts)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": ".*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  },
  "dependencies": {
    "reflect-metadata": "0.1.10"
  }
}
